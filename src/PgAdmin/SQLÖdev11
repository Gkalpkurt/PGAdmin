/* ============================
   Tekrarsız (DISTINCT) Veriler
   ============================ */

-- 1. actor ve customer tablolarındaki tüm first_name (UNION)

SELECT first_name FROM actor
UNION
SELECT first_name FROM customer;

-- 2. actor ve customer tablolarında ortak (kesişen) first_name (INTERSECT)

SELECT first_name FROM actor
INTERSECT
SELECT first_name FROM customer;

-- 3. actor tablosunda olup customer tablosunda olmayan first_name (EXCEPT)

SELECT first_name FROM actor
EXCEPT
SELECT first_name FROM customer;


/* ============================
   Tekrarlı (ALL) Veriler
   ============================ */

-- 4. actor ve customer tablolarındaki tüm first_name tekrarlar dahil (UNION ALL)

SELECT first_name FROM actor
UNION ALL
SELECT first_name FROM customer;

-- 5. actor ve customer tablolarında ortak (kesişen) first_name tekrarlar dahil (INTERSECT ALL)

SELECT first_name FROM actor
INTERSECT ALL
SELECT first_name FROM customer;

-- 6. actor tablosunda olup customer tablosunda olmayan first_name tekrarlar dahil (EXCEPT ALL)

SELECT first_name FROM actor
EXCEPT ALL
SELECT first_name FROM customer;